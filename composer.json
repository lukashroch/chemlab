{
    "name": "lukashroch/chemlab",
    "type": "project",
    "description": "Simple chemical inventory management",
    "keywords": [
        "chemlab",
        "chemical inventory"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Lukas Hroch",
            "email": "lukashroch@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "bepsvpt/secure-headers": "^6.3",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "ifsnop/mysqldump-php": "^2.9",
        "laravel/framework": "^8.0",
        "laravel/passport": "^10.0",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.4",
        "laravel/ui": "^3.0",
        "maatwebsite/excel": "^3.1",
        "martinlindhe/laravel-vue-i18n-generator": "@dev",
        "owen-it/laravel-auditing": "@dev",
        "santigarcor/laratrust": "^6.2",
        "swiftmailer/swiftmailer": "^6.2",
        "torann/geoip": "^3.0",
        "vladimir-yuldashev/flysystem-curlftp": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.3",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lukashroch/laravel-auditing.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/lukashroch/laravel-vue-i18n-generator.git"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "ChemLab\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "lang:gen": [
            "@php artisan vue-i18n:generate"
        ]
    }
}
