{
    "name": "lukashroch/chemlab",
    "type": "project",
    "description": "Simple chemical inventory management",
    "keywords": [
        "chemlab",
        "chemical inventory"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Lukas Hroch",
            "email": "lukashroch@hotmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-dom": "*",
        "ext-json": "*",
        "bepsvpt/secure-headers": "^7.4",
        "guzzlehttp/guzzle": "^7.8.1",
        "ifsnop/mysqldump-php": "^2.12",
        "laravel/framework": "^10.41",
        "laravel/passport": "^11.10.1",
        "laravel/socialite": "^5.11",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.4",
        "league/flysystem-ftp": "^3.22",
        "maatwebsite/excel": "^3.1.52",
        "martinlindhe/laravel-vue-i18n-generator": "@dev",
        "owen-it/laravel-auditing": "^13.6.4",
        "santigarcor/laratrust": "^8.2",
        "symfony/mailer": "^6.4.2",
        "torann/geoip": "^3.0.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23.1",
        "laravel/pint": "^1.13.9",
        "laravel/sail": "^1.27.1",
        "mockery/mockery": "^1.6.7",
        "nunomaduro/collision": "^7.10",
        "phpunit/phpunit": "^10.5.8",
        "spatie/laravel-ignition": "^2.4.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lukashroch/laravel-vue-i18n-generator.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "ChemLab\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
