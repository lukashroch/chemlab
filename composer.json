{
    "name": "lukashroch/chemlab",
    "type": "project",
    "description": "Simple chemical inventory management",
    "keywords": [
        "chemlab",
        "chemical inventory"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Lukas Hroch",
            "email": "lukashroch@hotmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-dom": "*",
        "ext-json": "*",
        "bepsvpt/secure-headers": "^7.3",
        "guzzlehttp/guzzle": "^7.5",
        "ifsnop/mysqldump-php": "^2.9",
        "laravel/framework": "^10.0",
        "laravel/passport": "^11.6",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "league/flysystem-ftp": "^3.10",
        "maatwebsite/excel": "^3.1",
        "martinlindhe/laravel-vue-i18n-generator": "@dev",
        "owen-it/laravel-auditing": "^13.0",
        "santigarcor/laratrust": "^7.1",
        "symfony/mailer": "^6.0",
        "torann/geoip": "^3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "laravel/pint": "^1.4",
        "laravel/sail": "^1.19",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lukashroch/laravel-vue-i18n-generator.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "ChemLab\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
