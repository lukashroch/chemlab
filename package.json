{
    "name": "chemlab",
    "version": "1.0.0",
    "description": "Simple chemical inventory management",
    "keywords": [
        "chemlab",
        "chemical inventory"
    ],
    "author": "Lukas Hroch",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lukashroch/chemlab.git"
    },
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "lint": "eslint --ext .js,.vue resources/js",
        "lint:fix": "npm run lint -- --fix",
        "locales": "php artisan vue-i18n:generate && eslint resources/js/locale/messages.js --fix"
    },
    "lint-staged": {
        "resources/js/**": "npm run lint"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.15.3",
        "axios": "^0.21.1",
        "bootstrap": "^4.6.0",
        "lodash": "^4.17.21",
        "vue": "^2.6.14",
        "vue-clipboard2": "^0.3.1",
        "vue-i18n": "^8.24.5",
        "vue-js-modal": "^1.3.35",
        "vue-ls": "^3.2.2",
        "vue-multiselect": "^2.1.6",
        "vue-router": "^3.5.2",
        "vue-scrollto": "^2.20.0",
        "vue-slide-up-down": "^2.0.1",
        "vue-toasted": "^1.1.28",
        "vue2-datepicker": "^3.9.1",
        "vuetable-2": "^2.0.0-beta.4",
        "vuex": "^3.6.2"
    },
    "devDependencies": {
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
        "@babel/plugin-proposal-optional-chaining": "^7.14.5",
        "babel-eslint": "^10.1.0",
        "babel-plugin-module-resolver": "^4.1.0",
        "core-js": "^3.15.2",
        "cross-env": "^7.0.3",
        "eslint": "^7.29.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-babel-module": "^5.3.1",
        "eslint-import-resolver-webpack": "^0.13.1",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-vue": "^7.12.1",
        "husky": "^7.0.0",
        "laravel-mix": "^6.0.25",
        "lint-staged": "^11.0.0",
        "prettier": "^2.3.2",
        "regenerator-runtime": "^0.13.7",
        "resolve-url-loader": "^4.0.0",
        "sass": "^1.35.1",
        "sass-loader": "^12.1.0",
        "vue-loader": "^15.9.7",
        "vue-template-compiler": "^2.6.14"
    }
}
