{
  "name": "chemlab",
  "version": "1.0.0",
  "description": "Simple chemical inventory management",
  "keywords": [
    "chemlab",
    "chemical inventory"
  ],
  "author": "Lukas Hroch",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukashroch/chemlab.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint resources/js --ext .js,.ts,.vue --fix",
    "locales": "php artisan vue-i18n:generate --format=json && prettier --write resources/js/i18n/messages.json",
    "type-check": "vue-tsc --noEmit"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "resources/js/*.{js,ts,vue}": "eslint --fix"
  },
  "dependencies": {
    "@bhplugin/vue3-datatable": "^1.1.3",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@popperjs/core": "^2.11.8",
    "@vueuse/core": "^10.7.2",
    "axios": "^1.6.7",
    "axios-retry": "^4.0.0",
    "bootstrap": "^5.3.2",
    "date-fns": "^3.3.1",
    "deepmerge": "^4.3.1",
    "fast-copy": "^3.0.1",
    "ketcher-core": "^2.15.1",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.5",
    "object-to-formdata": "^4.5.1",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "vue": "^3.4.15",
    "vue-final-modal": "^4.5.3",
    "vue-i18n": "^9.9.1",
    "vue-multiselect": "3.0.0-beta.3",
    "vue-router": "^4.2.5",
    "vue-scrollto": "^2.20.0",
    "vue3-slide-up-down": "^2.0.1"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@rushstack/eslint-patch": "^1.7.2",
    "@tsconfig/node18": "^18.2.2",
    "@types/bootstrap": "^5.2.10",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.21.1",
    "laravel-vite-plugin": "^1.0.1",
    "lint-staged": "^15.2.1",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-webfont-dl": "^3.9.1",
    "vue-template-compiler": "^2.7.16",
    "vue-tsc": "^1.8.27"
  }
